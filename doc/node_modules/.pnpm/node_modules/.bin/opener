#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/romainbourdain/Documents/stage-icube/althread-with-pest/althread/node_modules/.pnpm/opener@1.5.2/node_modules/opener/bin/node_modules:/Users/romainbourdain/Documents/stage-icube/althread-with-pest/althread/node_modules/.pnpm/opener@1.5.2/node_modules/opener/node_modules:/Users/romainbourdain/Documents/stage-icube/althread-with-pest/althread/node_modules/.pnpm/opener@1.5.2/node_modules:/Users/romainbourdain/Documents/stage-icube/althread-with-pest/althread/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/romainbourdain/Documents/stage-icube/althread-with-pest/althread/node_modules/.pnpm/opener@1.5.2/node_modules/opener/bin/node_modules:/Users/romainbourdain/Documents/stage-icube/althread-with-pest/althread/node_modules/.pnpm/opener@1.5.2/node_modules/opener/node_modules:/Users/romainbourdain/Documents/stage-icube/althread-with-pest/althread/node_modules/.pnpm/opener@1.5.2/node_modules:/Users/romainbourdain/Documents/stage-icube/althread-with-pest/althread/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../opener/bin/opener-bin.js" "$@"
else
  exec node  "$basedir/../opener/bin/opener-bin.js" "$@"
fi
